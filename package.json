{
  "name": "@kickstartds/storybook-addon-jsonschema",
  "version": "1.0.6",
  "description": "Show JSON Schema files associated with components, integrated with Story Controls",
  "keywords": [
    "storybook-addons",
    "data-state",
    "JSON",
    "JSON Schema",
    "Controls"
  ],
  "repository": {
    "type": "git",
