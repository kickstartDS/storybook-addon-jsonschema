    "auto": "^11.0.4",
    "decomment": "^0.9.5",
    "esbuild": "^0.19.3",
    "esbuild-node-externals": "^1.9.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "storybook": "^7.4.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  },
