      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "storybook": {
    "displayName": "JSON Schema",
    "supportedFrameworks": [
